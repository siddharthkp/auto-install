{
  "name": "auto-install",
  "version": "1.5.0",
  "description": "Auto installs dependencies as you code",
  "keywords": "auto, dependencies, install, package, watch",
  "homepage": "https://github.com/siddharthkp/auto-install#readme",
  "repository": "siddharthkp/auto-install",
  "license": "MIT",
  "main": "src/index.js",
  "bin": "lib/index.js",
  "author": "siddharthkp",
  "scripts": {
    "lint": "node node_modules/eslint/bin/eslint.js src",
    "build": "babel src -d lib && babel test/src -d test/lib",
    "run-tests": "cd example && node ../node_modules/.bin/mocha ../test/lib",
    "test": "npm run lint && npm run build && npm run run-tests"
  },
  "engines": {
    "node": ">= 4.4.5"
  },
  "dependencies": {
    "chokidar": "1.6.1",
    "colors": "1.1.2",
    "detective": "4.3.2",
    "glob": "7.1.0",
    "is-builtin-module": "1.0.0",
    "log-symbols": "1.0.2",
    "ora": "0.3.0",
    "package-json": "2.4.0",
    "request": "2.74.0",
    "sync-exec": "0.6.2",
    "yargs": "6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.2.2",
    "mocha": "^3.0.2"
  },
  "babel": {
    "presets": ["es2015"]
  }
}
